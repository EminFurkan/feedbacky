{
    "version": 2,
    "builds": [
        {
            "src": "dist/index.js",
            "use": "@vercel/node",
            "config": {
                "includeFiles": [
                    "dist/**"
                ]
            }
        },
        {
            "src": "public/**",
            "use": "@vercel/static"
        }
    ],
    "routes": [
        {
            "src": "/(.*)",
            "dest": "dist/index.js"
        },
        {
            "src": "/lib.js",
            "dest": "public/lib.js"
        },
        {
            "src": "/(.+)",
            "dest": "public/$1"
        }
    ]
} 